        ".h",
        ".ii",
        ".ino",
        ".inl",
        ".ipp",
        ".ixx",
        ".tpp",
        ".txx",
        ".hpp.in",
        ".h.in"
      ],
      "configuration": "./languageConfiguration.json",
      "tokenize": "src/tokenizeCpp.js"
    }
  ]
}
